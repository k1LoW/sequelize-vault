{
  "name": "sequelize-vault",
  "version": "1.4.0",
  "description": "A Sequelize plugin for easily integrating Hashicorp Vault",
  "main": "index.js",
  "types": "lib/sequelize-vault.d.ts",
  "files": [
    "LICENSE",
    "README.md",
    "index.js",
    "lib"
  ],
  "directories": {
    "lib": "lib",
    "src": "src"
  },
  "dependencies": {
    "@types/node": "^10.0.6",
    "axios": "^0.18.0",
    "sequelize": "^4.37.7"
  },
  "devDependencies": {
    "sequelize-typescript": "^0.6.4",
    "reflect-metadata": "^0.1.12",
    "ava": "^0.25.0",
    "coveralls": "^3.0.1",
    "nsp": "^3.2.1",
    "nyc": "^11.7.3",
    "sqlite3": "^4.0.0",
    "tslint": "^5.10.0",
    "tslint-microsoft-contrib": "^5.0.3",
    "typescript": "^2.8.3",
    "testdouble": "^3.8.1"
  },
  "engines": {
    "node": ">=8.4.0"
  },
  "ava": {
    "files": [
      "lib/**/*.test.js"
    ]
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf lib",
    "ci": "npm install && npm run lint && npm run cov && nsp check",
    "test": "npm run build && ava -v",
    "cov": "npm run build && nyc ava",
    "lint": "tslint -p tsconfig.json -c tslint.json src/**/*.ts",
    "preversion": "npm run clean && npm run build && npm test",
    "postversion": "git push origin --tags",
    "prepublishOnly": "npm run clean && npm run build && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/linyows/sequelize-vault.git"
  },
  "keywords": [
    "sequelize",
    "vault"
  ],
  "author": "linyows",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/linyows/sequelize-vault/issues"
  },
  "homepage": "https://github.com/linyows/sequelize-vault"
}
